{
    "family": "af-back",
    "executionRoleArn": "arn:aws:iam::430278725858:role/akila-test",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "af-back",
            "image": "public.ecr.aws/i7k0q0b2/af-back:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential" : true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": ".25 vCPU",
    "memory": "0.5GB"
}